package com.skslate;

public class LakshmiAirtelService {
	private String area ;
	private int noOfTowers;
	private AirtelStore airtelStores;

	public AirtelStore getAirtelStores() {
		return airtelStores;
	}

	public void setAirtelStores(AirtelStore airtelStores) {
		this.airtelStores = airtelStores;
	}

	public void setArea(String area) {
		this.area = area;
	}

	public void setNoOfTowers(int noOfTowers) {
		this.noOfTowers = noOfTowers;
	}

	public String getArea() {
		return area;
	}

	public int getNoOfTowers() {
		return noOfTowers;
	}
}
