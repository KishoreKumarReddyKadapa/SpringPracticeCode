package com.skslate.autowire;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.beans.factory.annotation.Qualifier;

public class Airtel implements Sim {
	
	private String area ;
	private int noOfTowers;
	private AirtelStore airtelStores;

	public AirtelStore getAirtelStores() {
		return airtelStores;
	}

	@Autowired 
	public void setAirtelStores(AirtelStore airtelStores) {
		this.airtelStores = airtelStores;
		System.out.println("Setter called...");
	}

	public void setArea(String area) {
		this.area = area;
	}

	public void setNoOfTowers(int noOfTowers) {
		this.noOfTowers = noOfTowers;
	}

	public String getArea() {
		return area;
	}

	public int getNoOfTowers() {
		return noOfTowers;
	}

	@Override
	public void calling() {
		System.out.println("Airtel Sim Calling");
		
	}

	@Override
	public void messaging() {
		System.out.println("Airtel Sim messaging");
		
	}

	@Override
	public void browse() {
		System.out.println("Airtel Sim browse");
		
	}
	
	

}
